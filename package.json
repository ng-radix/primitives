    "scripts": {
        "----Storybook----": "-----------------------------------------------------------------------",
        "storybook:primitives": "pnpm run compodoc:json && nx run primitives:storybook",
        "storybook:components": "nx run components:storybook",
        "build-storybook": "pnpm run compodoc:json && nx run primitives:build-storybook",
        "----showcases----------": "-----------------------------------------------------------------",
        "showcase-taxonomy:serve": "nx serve showcase-taxonomy",
        "showcase-taxonomy:build": "nx build showcase-taxonomy",
        "showcase-dashboard:serve": "nx serve showcase-dashboards",
        "showcase-dashboard:build": "nx build showcase-dashboards",
        "----Primitives---": "-----------------------------------------------------------------------",
        "primitives:build": "nx run primitives:build",
        "primitives:test": "nx run primitives:test",
        "compodoc:json": "compodoc -p ./packages/primitives/tsconfig.json -e json -d ./packages/primitives/.storybook",
        "----Components---": "-----------------------------------------------------------------------",
        "components:build": "nx run components:build",
        "----shadcn-------": "-----------------------------------------------------------------------",
        "shadcn:build": "nx run shadcn:build",
        "----Others-------": "-----------------------------------------------------------------------",
        "prepare": "husky",
        "format:check": "prettier --check packages/**/** --cache",
        "format:write": "prettier --write packages/**/** --cache"
    },
