---
import { DemoLoaderComponent } from '../demo-primitive-preview/demo-loader.component';
export interface Props {
    componentName?:
        | 'accordion'
        | 'avatar'
        | 'alert-dialog'
        | 'checkbox'
        | 'progress'
        | 'collapsible'
        | 'dropdown-menu'
        | 'default';
    componentFile?: string;
    overflow?: boolean;
}

const { componentName, componentFile } = Astro.props;
---

<div class="light-theme" data-component={componentName}>
    <DemoLoaderComponent {...{ name: componentName, file: componentFile }} client:visible />
</div>
